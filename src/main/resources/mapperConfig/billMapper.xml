<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.murong.wjl.testspringboot2.mapper.BillMapper">
   <resultMap id="BillList" type="com.murong.wjl.testspringboot2.domain.Bill" >
      <result column="JRN_NO" property="jrnNo" />
      <result column="TX_TYP" property="txTyp" />
      <result column="REMARK" property="remark" />
   </resultMap>


   <select id="getBill" resultType="com.murong.wjl.testspringboot2.domain.Bill">
      SELECT * FROM bill WHERE no = #{jrnNo}
   </select>

   <select id="getAll" resultMap="BillList">
      SELECT * FROM bill
   </select>
</mapper>